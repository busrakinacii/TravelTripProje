@model TravelTripProje.Models.Classes.communication

@{
    ViewBag.Title = "MesajGetir";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2 style="margin-bottom:25px; margin-top:25px">Mesaj Detay Sayfası</h2>
<form class="form-group" method="post">
    @Html.LabelFor(x => x.ID, new { @style = "margin-bottom:10px" })
    @Html.TextBoxFor(x => x.ID, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.AdSoyad, new { @style = "margin-bottom:10px" })
    @Html.TextBoxFor(x => x.AdSoyad, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.Mail, new { @style = "margin-bottom:10px" })
    @Html.TextBoxFor(x => x.Mail, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.Konu, new { @style = "margin-bottom:10px" })
    @Html.TextBoxFor(x => x.Konu, new { @class = "form-control", @readonly = "readonly" })
    <br />
    @Html.LabelFor(x => x.Mesaj, new { @style = "margin-bottom:10px" })
    @Html.TextAreaFor(x => x.Mesaj, new { @class = "form-control", rows = "7", cols = "70", @readonly = "readonly" })
    <br />
    <a href="/Admin/MesajListesi/" class="btn btn-info"><b><<</b></a>
</form>



